import java.util.Scanner;

public class Problem1A {
	public static void main(String[] args) {
		Scanner sample = new Scanner(System.in);
		double n = sample.nextInt();
		double m = sample.nextInt();
		double a = sample.nextInt();
		double na = Math.ceil(n/a);
		double ma = Math.ceil(m/a);
		System.out.println((long)(na*ma));
	}

}
